use std::net::TcpListener;

fn main() {
    let listener = TcpListener::bind("127.0.0.1:1664").unwrap();
    for connection in listener.incoming() {
        match connection {
       	    Ok(con) => {
	     	println!("Connection!");
	    }
	    Err(e) => {
	        println!("Shite");
	    }
        }
    }
}
